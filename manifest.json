{
  "manifest_version": 2,
  "name": "Roblox 40% Saver",
  "version": "1.2",
  "description": "Save 40% on roblox accesories.",
  "permissions": ["activeTab", "storage", "webNavigation"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://www.roblox.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
